{
  "compilerOptions": {
    "target": "ES2021",
    "moduleResolution": "node",
    "baseUrl": "./src",
    // "paths": {
    //   "@config/*": ["./config/*"],
    //   "@infra/*": ["./infra/*"],
    //   "@module/*": ["./modules/*"]
    // },
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  }
}
